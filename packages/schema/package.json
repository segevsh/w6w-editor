{
    "name": "@w6w-io/schema",
    "version": "1.0.0-alpha.1",
    "description": "JSON Schema definitions for W6W (Workflow) components and configurations",
    "type": "module",
    "main": "./src/index.ts",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts"
        },
        "./workflow.ts": {
            "types": "./src/workflow.ts",
            "import": "./src/workflow.ts"
        },
        "./node.ts": {
            "types": "./src/node.ts",
            "import": "./src/node.ts"
        },
        "./edge.ts": {
            "types": "./src/edge.ts",
            "import": "./src/edge.ts"
        },
        "./variable.ts": {
            "types": "./src/variable.ts",
            "import": "./src/variable.ts"
        },
        "./execution-log.ts": {
            "types": "./src/execution-log.ts",
            "import": "./src/execution-log.ts"
        }
    },
    "scripts": {
        "build": "echo 'No build step needed - exporting Zod schemas directly'"
    },
    "dependencies": {
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "tsx": "^4.19.2"
    }
}